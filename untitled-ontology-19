        <rdfs:range>
            <owl:Class>
                <owl:complementOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item"/>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>itemContent - The link to the actual resource to which the item refers.</rdfs:comment>
        <rdfs:label>item content</rdfs:label>
    </owl:ObjectProperty>
    


